package com.pkart.dao;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

import com.pkart.model.Product;

public class DowTest {
	ICartDao cartDao = new CartDaoImpl();
	IProductDao productDao = new ProductDaoImpl();
	
	@Before
    public void init() {
        
        cartDao.addItem(1, new Product(2,"Fan",1200));
        System.out.println(cartDao.viewCart(1));
        
    }
	
	
	@Test
	public void test() {
		assertEquals("Cart [id=1, products=[Product [id=2, name=Fan, price=1200.0, quantity=0, dateOfManufacture=null, dateOfExpiry=null]]]\r\n"
				+ "",cartDao.viewCart(1));
		
	}

}
